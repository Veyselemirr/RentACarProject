@model CreateBrandDto

@{
    ViewData["Title"] = "CreateBrand";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Araba Markası Ekleme Sayfası</h4>
                                <form method="post">
                                    <div class="form-group">
                                        <label for="exampleInputName">Özellik</label>
                                        <input type="text" class="form-control" id="exampleInputName" asp-for="name" placeholder="Araç Markası Giriniz">
                                    </div>

                                    <button type="submit" class="btn btn-primary waves-effect waves-light">Marka Ekle</button>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>













